{
    "name": "ppi/framework",
    "description": "The PPI Framework User Module",
    "homepage": "http://www.ppi.io",
    "license": "MIT",
    "keywords": ["framework", "user"],
    "type": "library",
    "authors": [
        {
            "name": "Paul Dragoonis",
            "email": "paul@ppi.io"
        }
    ],
    "target-dir": "modules/User",
    "require": {
        "ppi/zendframework": "2.0.*@beta"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ppi/zendframework",
                "version": "2.0.0-beta5",
                "dist": {
                    "url": "http://packages.zendframework.com/releases/ZendFramework-2.0.0beta5/ZendFramework-2.0.0beta5-minimal.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": { "Zend": "library" }
                }
            }
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}